S -> Inst

Inst -> Asig Inst | Ejec Inst | eps
Asig -> VARIABLE '=' Ejec

Vars_2 -> VARIABLE ',' Vars_2
Vars_2 -> VARIABLE

Ejec -> Bagging

Bagging -> BAGGING '(' '[' Vars_2 ']' ',' 'Baggin_Mode' ')'
Bagging_Mode -> MEAN | MODE


MEAN = r'((?<![a-zA-Z0-9])(mean|Mean)(?![a-zA-Z0-9]))'
MODE = r'((?<![a-zA-Z0-9])(mode|Mode)(?![a-zA-Z0-9]))'